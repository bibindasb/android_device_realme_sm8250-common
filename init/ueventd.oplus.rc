# Charger
/sys/class/oplus_chg/*  mmi_charging_enable    0660    system  system

# NFC
/dev/st21nfc    0660    nfc               nfc
/dev/st54spi    0660    secure_element    secure_element

# Reserve
/dev/block/bootdevice/by-name/oplusreserve1    0660    root    system
/dev/block/bootdevice/by-name/oplusreserve3    0660    root    system
/dev/block/sde13                               0660    root    system
/dev/block/sde15                               0660    root    system
/dev/block/sde50                               0660    root    system
/dev/block/sdf10                               0660    root    system
/dev/block/sdf13                               0660    root    system
/dev/block/sdf15                               0660    root    system



# WiFi
firmware_directories /mnt/vendor/persist/copy/ /mnt/vendor/persist/ /odm/etc/wifi/ /vendor/firmware_mnt/image/qca6390/ /vendor/firmware_mnt/image/qca6490/ /vendor/firmware_mnt/image/
